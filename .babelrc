{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions",
                        "safari >= 7"
                    ]
                }
            }
        ],
        "react",
        "es2015",
        "stage-0",
        "flow"
    ],
    "plugins": [
        [
            "transform-runtime",
            "transform-es2015-arrow-functions",
            {
                "spec": true
            }
        ]
    ]
}